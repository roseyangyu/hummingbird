<launch>
<arg name="ns"/>
<arg name="mavlink_fcu_url"/>

<group ns="$(arg ns)">
	<node pkg="joy" name="joy_node" type="joy_node"/>
	<node pkg="hummingbird_px4" name="manual_input" type="manual_input"/>
	<node pkg="hummingbird_px4" name="commander" type="commander"/>
	<node pkg="hummingbird_px4" name="mc_mixer" type="mc_mixer"/>
	<node pkg="hummingbird_px4" name="attitude_estimator" type="attitude_estimator"/>
	<node pkg="hummingbird_px4" name="position_estimator" type="position_estimator"/>
	<node pkg="hummingbird_px4" name="mc_att_control" type="mc_att_control"/>
	<node pkg="hummingbird_px4" name="mc_pos_control" type="mc_pos_control"/>
	<node pkg="hummingbird_px4" name="mavlink" type="mavlink">
	  <param name="mavlink_fcu_url" value="$(arg mavlink_fcu_url)" type="str"/>
	</node>
	<!-- <node pkg="rosbag" type="record" name="record" output="screen" args="-a -O px4_multicopter"/> -->
</group>

</launch>
